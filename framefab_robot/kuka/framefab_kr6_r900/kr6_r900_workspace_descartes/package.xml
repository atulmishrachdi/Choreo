<?xml version="1.0"?>
<package format="2">
  <name>kr6_r900_workspace_descartes</name>
  <version>0.1.0</version>
  <description>
    Defines a Descartes RobotModel plugin using the framefab_kr6_r900_ikfast_manipulator_plugin
    for kinematics. 
  </description>

  <maintainer email="yijiangh@mit.edu">Yijiang Huang</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>descartes_core</depend>
  <depend>descartes_moveit</depend>
  <depend>kr6_r900_workspace_ikfast_manipulator_plugin</depend>
  <depend>pluginlib</depend>

  <export>
    <descartes_core plugin="${prefix}/kr6_r900_workspace_descartes_plugins.xml"/>
  </export>

</package>
